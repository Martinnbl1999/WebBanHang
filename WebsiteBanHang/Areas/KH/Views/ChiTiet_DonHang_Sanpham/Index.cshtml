@model IEnumerable<WebsiteBanHang.Models.ChiTiet_DonHang_Sanpham>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/KH/Views/Shared/TrangpageconKH.cshtml";
}

<table class="content-table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.soluong)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dongia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DonHang.madonhang)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SanPham.tensanpham)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.soluong)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dongia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DonHang.madonhang)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SanPham.tensanpham)
        </td>
        
    </tr>
}

</table>
